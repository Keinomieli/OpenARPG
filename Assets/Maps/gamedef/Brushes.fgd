		128 : "Invisible" : 0
	]
	killtarget(target_destination) : "KillTarget"
	damage_type(choices) : "Damage Type" : 0 =
	[
		0 : "Based on material"
		1 : "Fire"
		2 : "Cold"
		3 : "Poison"
	]
	damagePerTick(integer) : "Damage Per Tick" : 10
	ticksPerSecond(integer) : "Ticks Per Second" : 4
	CollisionLayer(Flags) =
	[
		1 : "1" : 0
		2 : "2" : 0
		4 : "3" : 0
		8 : "4" : 0
		16 : "5" : 0
		32 : "6" : 0
		64 : "7" : 0
		128 : "8" : 0
		256 : "9" : 0
		512 : "10" : 0
		1024 : "11" : 0
		2048 : "12" : 0
	]
	CollisionMask(Flags) =
	[
		1 : "1" : 0
		2 : "2" : 0
		4 : "3" : 0
		8 : "4" : 0
		16 : "5" : 0
		32 : "6" : 0
		64 : "7" : 0
		128 : "8" : 0
		256 : "9" : 0
		512 : "10" : 0
		1024 : "11" : 0
		2048 : "12" : 0
	]
]

@SolidClass base(EditorGizmos, SpecialBrush) color(0 255 255) = door_concave : "Door (concave)"
[
	visible_name(string) : "Visible Name" : "Door (concave)"
	assembly_type(string) : "Assembly Type" : "Door"
	brush_type(string) : "Brush Type" : "ConcaveStaticBody"
]

@SolidClass base(EditorGizmos, SpecialBrush) color(0 255 255) = door_convex : "Door (convex)"
[
