@baseclass = Target [ target(target_destination) : "Target" ]

@baseclass = EditorGizmos
[
	visible_name(string) : "Visible Name"
]

@baseclass = Pivot
[
	pivot(choices) : "Pivot" : 1 =
	[
		0 : "Root"
		1 : "Center"
		2 : "Bottom"
		3 : "Top"
	]
]

@baseclass = Normals
[
	normals(choices) : "Normals" : 0 =
	[
		0 : "Faceted"
		1 : "Smooth"
	]
]

@baseclass base(Pivot, Normals) = SpecialBrush
[
	brush_type(string) : "Brush Type"
	assembly_type(string) : "Assembly Type"
]

@SolidClass base(Pivot, Normals) = Common_StaticConcave
[
	brush_type(string) : "Brush Type" : "ConcaveStaticBody"
]

@SolidClass base(Pivot, Normals) = Common_StaticConvex
[
	brush_type(string) : "Brush Type" : "ConvexStaticBody"
]

@baseclass base(SpecialBrush) = Trigger
[
	brush_type(string) : "Brush Type" : "ConvexCollider"
	sounds(choices) : "Sound style" : 0 =
	[
		0 : "None"
		1 : "Secret sound"
		2 : "Beep beep"
		3 : "Large switch"
	]
	delay(string) : "Delay before trigger" : "0"
	message(string) : "Message"
]

@SolidClass base(EditorGizmos, Appearflags, Trigger) color(255 0 0) = trigger_hurt : "Hurt Trigger"
[
	visible_name(string) : "Visible Name" : "Hurt Trigger"
	assembly_type(string) : "Assembly Type" : "HurtArea"
	spawnflags(Flags) =
